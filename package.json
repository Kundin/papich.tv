    "scripts": {
        "dev": "cross-env BABEL_ENV=node NODE_ENV=development nodemon src/server/bootstrap.js",
        "prod": "cross-env NODE_ENV=production node lib/server/index.js",
        "build": "cross-env NODE_ENV=production yarn build:webpack && yarn build:lib",
        "build:webpack": "webpack",
        "build:lib": "babel -d lib src",
        "remove-statics": "rm -rf public/tmp/*.* && rm -rf public/files/*.*",
        "gql-analize": "node src/server/graphql/analize.js"
    },
